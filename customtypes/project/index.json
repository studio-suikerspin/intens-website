			"location": {
				"type": "Text",
				"config": {
					"label": "Location",
					"placeholder": "Amsterdam"
				}
			},
			"project_date": {
				"type": "Date",
				"config": {
					"label": "Project date",
					"placeholder": ""
				}
			},
			"project_type": {
				"type": "Text",
				"config": {
					"label": "Project type",
					"placeholder": "BKT Renovatie"
				}
			},
			"summary": {
				"type": "Text",
