{
	"format": "custom",
	"id": "project",
	"label": "Project",
	"repeatable": true,
	"status": true,
	"json": {
		"Main": {
			"uid": {
				"type": "UID",
				"config": {
					"label": "UID"
				}
			},
			"title": {
				"type": "Text",
				"config": {
					"label": "Title",
					"placeholder": "Project title"
				}
			},
			"location": {
				"type": "Text",
				"config": {
					"label": "Location",
					"placeholder": "Amsterdam"
				}
			},
			"project_date": {
				"type": "Date",
				"config": {
					"label": "Project date",
					"placeholder": ""
				}
			},
			"project_type": {
				"type": "Text",
				"config": {
					"label": "Project type",
					"placeholder": "BKT Renovatie"
				}
			},
			"summary": {
				"type": "Text",
				"config": {
					"label": "Summary",
					"placeholder": "Summary in one or two sentences. Make sure this isn't too long!"
				}
			},
			"content_1": {
				"type": "StructuredText",
				"config": {
					"label": "content_1",
					"placeholder": "First content section goes here",
					"allowTargetBlank": true,
					"multi": "paragraph,preformatted,strong,em,hyperlink,embed,list-item,o-list-item"
				}
			},
			"content_2": {
				"type": "StructuredText",
				"config": {
					"label": "content_2",
					"placeholder": "Second content section goes here",
					"allowTargetBlank": true,
					"multi": "paragraph,preformatted,strong,em,hyperlink,embed,list-item,o-list-item"
				}
			},
			"featured_image": {
				"type": "Image",
				"config": {
					"label": "featured_image",
					"constraint": {},
					"thumbnails": []
				}
			},
			"gallery": {
				"type": "Group",
				"config": {
					"label": "Gallery",
					"repeat": true,
					"fields": {
						"item_content": {
							"type": "Link",
							"config": {
								"label": "Item content",
								"placeholder": "",
								"select": "media",
								"allowText": true
							}
						}
					}
				}
			},
			"tags": {
				"type": "Group",
				"config": {
					"label": "Tags",
					"repeat": true,
					"fields": {
						"project_tag": {
							"type": "Link",
							"config": {
								"label": "Project tag",
								"select": "document",
								"repeat": false,
								"customtypes": [
									{
										"id": "project_tag",
										"fields": ["tag"]
									}
								]
							}
						}
					}
				}
			}
		}
	}
}
