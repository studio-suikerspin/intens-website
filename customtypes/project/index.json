					"choices": {
						"side_by_side_media_slider": {
							"type": "SharedSlice"
						},
						"image_gallery_two_column": {
							"type": "SharedSlice"
						},
						"text_only": {
							"type": "SharedSlice"
						},
						"logo_grid": {
							"type": "SharedSlice"
						},
						"navigational_media_grid": {
							"type": "SharedSlice"
						},
						"hero_with_navigation_overlay": {
							"type": "SharedSlice"
						}
