			"uid": {
				"type": "UID",
				"config": {
					"label": "UID"
				}
			},
			"tags": {
				"type": "Group",
				"config": {
					"label": "Tags",
					"repeat": true,
					"fields": {
						"project_tag": {
							"type": "Link",
							"config": {
								"label": "Project tag",
								"select": "document",
								"repeat": false,
